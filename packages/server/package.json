{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.10",
    "@graphql-tools/schema": "^8.3.10",
    "apollo-server-core": "^3.7.0",
    "apollo-server-express": "^3.7.0",
    "argon2": "^0.28.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-jwt": "^7.6.2",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.8.1",
    "jsonwebtoken": "^8.5.1",
    "logform": "^2.4.0",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.7",
    "winston": "^3.7.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.10",
    "@graphql-codegen/typescript-resolvers": "^2.6.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/sqlite3": "^3.1.8",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "utility-types": "^3.10.0"
  },
  "scripts": {
    "start": "pnpm build && node ./dist/index.js",
    "start:dev": "concurrently \"pnpm dev:app\" \"pnpm graphql:generate --watch\"",
    "dev:app": "nodemon --exec 'babel-node --extensions \".ts\" ./src/index.ts'",
    "build": "pnpm graphql:generate && pnpm build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps true",
    "test": "nyc mocha tests",
    "prettier": "prettier --check '{src,tests}/**/*.*'",
    "prettier:write": "prettier --write '{src,tests}/**/*.*'",
    "lint": "eslint '{src,tests}/**/*.{j,t}s{,x}'",
    "lint:fix": "eslint --fix '{src,tests}/**/*.{j,t}s{,x}'",
    "graphql:generate": "graphql-codegen"
  }
}
