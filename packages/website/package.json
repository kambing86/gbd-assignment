{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "clsx": "^1.1.1",
    "graphql-ws": "^5.7.0",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "3.1.4",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "yarn graphql:download && concurrently \"yarn start\" \"yarn graphql:generate --watch\"",
    "build": "yarn graphql:generate && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --check 'src/**/*.*'",
    "prettier:write": "prettier --write 'src/**/*.*'",
    "lint": "eslint 'src/**/*.{j,t}s{,x}'",
    "lint:fix": "eslint --fix 'src/**/*.{j,t}s{,x}'",
    "graphql:download": "rover graph introspect http://localhost:8010/graphql > schema.gql",
    "graphql:generate": "graphql-codegen"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/rover": "^0.5.1",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-react-apollo": "^2.3.1",
    "@types/lodash": "^4.14.181",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "utility-types": "^3.10.0"
  }
}
