    "typescript": "~3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "concurrently \"yarn start\" \"yarn dev:graphql\"",
    "dev:graphql": "yarn generate --watch",
    "prebuild": "yarn generate",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --check 'src/**/*.*'",
    "prettier:write": "prettier --write 'src/**/*.*'",
    "lint": "eslint 'src/**/*.{j,t}s{,x}'",
    "lint:fix": "eslint --fix 'src/**/*.{j,t}s{,x}'",
    "generate": "graphql-codegen"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.17.7",
    "@graphql-codegen/cli": "^1.17.7",
    "@graphql-codegen/typescript": "^1.17.7",
    "@graphql-codegen/typescript-operations": "^1.17.7",
    "@graphql-codegen/typescript-react-apollo": "^1.17.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/recharts": "^1.8.14",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^5.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "utility-types": "^3.10.0"
  }
