{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.6.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.4",
    "@mui/styles": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "clsx": "^1.1.1",
    "graphql-ws": "^5.8.1",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "3.1.4",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "yarn graphql:download && concurrently \"yarn start\" \"yarn graphql:generate --watch\"",
    "build": "yarn graphql:generate && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --check 'src/**/*.*'",
    "prettier:write": "prettier --write 'src/**/*.*'",
    "lint": "eslint 'src/**/*.{j,t}s{,x}'",
    "lint:fix": "eslint --fix 'src/**/*.{j,t}s{,x}'",
    "graphql:download": "rover graph introspect http://localhost:8010/graphql > schema.gql",
    "graphql:generate": "graphql-codegen"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/rover": "^0.6.0",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.10",
    "@graphql-codegen/typescript-operations": "^2.3.7",
    "@graphql-codegen/typescript-react-apollo": "^3.2.13",
    "@types/lodash": "^4.14.182",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "utility-types": "^3.10.0"
  }
}
